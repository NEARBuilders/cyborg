{
  "account": "example.near",
  "create": {
    "project": "nearbuilders/cyborg",
    "ui": "nearbuilders/cyborg/ui",
    "api": "nearbuilders/cyborg/api",
    "host": "nearbuilders/cyborg/host"
  },
  "app": {
    "host": {
      "title": "NEAR Agent",
      "description": "Template for building NEAR AI applications",
      "development": "http://localhost:3000",
      "production": "https://near-agent.pages.dev",
      "secrets": [
        "HOST_DATABASE_URL",
        "HOST_DATABASE_AUTH_TOKEN",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL"
      ]
    },
    "ui": {
      "name": "ui",
      "development": "http://localhost:3002",
      "production": "https://near-agent.pages.dev",
      "exposes": {
        "App": "./App",
        "components": "./components",
        "providers": "./providers",
        "types": "./types"
      },
      "ssr": "https://near-agent.pages.dev"
    },
    "api": {
      "name": "api",
      "development": "http://localhost:3014",
      "production": "https://near-agent.pages.dev/api",
      "variables": {
        "NEAR_AI_MODEL": "deepseek-ai/DeepSeek-V3.1",
        "NEAR_RPC_URL": "https://rpc.mainnet.near.org",
        "NEAR_LEGION_CONTRACT": "ascendant.nearlegion.near",
        "NEAR_INITIATE_CONTRACT": "initiate.nearlegion.near"
      },
      "secrets": [
        "API_DATABASE_URL",
        "API_DATABASE_AUTH_TOKEN",
        "NEAR_AI_API_KEY",
        "NEAR_AI_BASE_URL"
      ]
    }
  }
}
